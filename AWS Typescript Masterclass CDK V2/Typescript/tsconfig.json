{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ES6",
      "ES2015"
    ],
    "target": "ES2019",
    "rootDir": "src",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "@components/*":["data/components/*"]
    },
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  }
}